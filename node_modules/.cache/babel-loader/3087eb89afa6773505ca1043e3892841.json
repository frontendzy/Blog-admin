l-table :data=\"data\" style=\"width: 100%\">\n    <el-table-column prop=\"_id\" label=\"ID\"></el-table-column>\n    <el-table-column prop=\"title\" label=\"标题\"></el-table-column>\n    <el-table-column prop=\"belong.classification\" label=\"分类\"></el-table-column>\n     <el-table-column fixed=\"right\" label=\"操作\" width=\"200\">\n      <template slot-scope=\"scope\">\n        <el-button @click=\"$router.push(`/admin/article/edit/${scope.row._id}`)\" size=\"small\" type=\"primary\">编辑</el-button>\n        <el-button @click=\"remove(scope.row)\" size=\"small\" type=\"danger\">删除</el-button>\n      </template>\n    </el-table-column>\n  </el-table>\n  </div>\n</template>\n\n<script>\nexport default {\n  data() {\n    return {\n      data: []\n    }\n  },\n  methods: {\n    async fetch() {\n      const res = await this.$http.get('rest/article');\n      this.data = res.data;\n    },\n    async remove(row) {\n      this.$confirm('此操作将永久删除该分类, 是否继续?', '提示', {\n          confirmButtonText: '确定',\n          cancelButtonText: '取消',\n          type: 'warning'\n        })\n        .then(async () => {\n          const res = await this.$http.delete(`rest/article/${row._id}`)\n          this.$message({\n            type: 'success',\n            message: '删除成功!'\n          });\n          this.fetch();\n        })\n    }\n  },\n  created() {\n    this.fetch();\n  }\n}\n</script>\n\n<style scoped lang=\"css\">\n\n</style>\n"],"sourceRoot":"src/views/pages/article"}]}